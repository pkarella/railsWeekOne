<%= render('layouts/errors', :object => @review) %>

<%= form_for [@product, @review] do |f| %>
<%= f.label :author %>
<%= f.text_field :author %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <p><%= f.submit %></p>
<% end %>
